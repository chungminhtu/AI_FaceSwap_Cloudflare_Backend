{
  "name": "ai-faceswap-cloudflare-backend",
  "version": "1.0.0",
  "description": "Cloudflare Worker backend and Pages frontend for AI FaceSwap application with GCP integration",
  "main": "backend-cloudflare-workers/index.ts",
  "scripts": {
    "deploy:ai-office": "DEPLOY_ENV=ai-office node _deploy-cli-cloudflare-gcp/deploy.js",
    "db:migrate:ai-office": "DEPLOY_ENV=ai-office npm run db:migrate",
    "------": "wrangler dev",
    "deploy:ai-office-dev": "DEPLOY_ENV=ai-office-dev node _deploy-cli-cloudflare-gcp/deploy.js",
    "db:migrate:ai-office-dev": "DEPLOY_ENV=ai-office-dev npm run db:migrate",
    "deploy:ai-office-prod": "DEPLOY_ENV=ai-office-prod node _deploy-cli-cloudflare-gcp/deploy.js",
    "db:migrate": "node _deploy-cli-cloudflare-gcp/deploy.js --migrate-only",
    "docs:build": "node frontend-cloudflare-pages/docs/build-static.js",
    "deploy:parallel": "node _deploy-cli-cloudflare-gcp/deploy.js --envs",
    "deploy:all": "node _deploy-cli-cloudflare-gcp/deploy.js --all-envs",
    "deploy:parallel:test": "npm run docs:build && node _deploy-cli-cloudflare-gcp/deploy.js --envs ai-office,ai-office-dev,ai-office-prod",
    "generate:api-key": "node backend-cloudflare-workers/generate-api-key.js"
  },
  "dependencies": {
    "express": "^4.18.2",
    "nanoid": "^5.0.0",
    "jszip": "^3.10.1"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20241106.0",
    "@types/jszip": "^3.10.1",
    "typescript": "^5.3.3",
    "wrangler": "^4.47.0"
  }
}
